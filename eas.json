{
  "cli": {
    "version": ">= 12.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_SERVER_URL":"http://localhost:8080"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6689520501"
      }
    }
  }
}
